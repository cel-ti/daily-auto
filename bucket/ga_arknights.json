    "$configContent = '{\"VersionUpdate.AutoDownloadUpdatePackage\": \"False\", \"VersionUpdate.AutoInstallUpdatePackage\": \"False\"}'",
    "Set-Content -Path \"$persist_dir\\config\\gui.json\" -Value $configContent"
  ]
